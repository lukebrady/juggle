{
    // HTTP or HTTPS protocol.
    "protocol" : "http",
    // List of load-balanced hosts.
    "hosts" : 
    [
        "localhost:8090",
        "localhost:8091",
        "localhost:8092"
    ],
    /*
    Available Algorithms:
    - Round Robin: rr
    - Least Loaded: ll
    - Priority: pr
    */
    "algorithm" : "rr",
    // Default port 9080
    "port" : ":9080"
}